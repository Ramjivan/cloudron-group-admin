# Cloudron Server Configuration
CLOUDRON_API_URL="https://my.cloudron.server"
CLOUDRON_API_TOKEN="your_cloudron_api_token"
CLOUDRON_GROUP_NAME="your_target_group_name"

# Dashboard Authentication
DASHBOARD_USERNAME="admin"
DASHBOARD_PASSWORD="a_very_strong_password"

# Mailbox Domains (comma-separated)
MAIL_DOMAINS="example.com,another-domain.org"

# Branding
BRAND_NAME="Cloudron"

# Exclusions - these accounts will not be visible in the UI even if they are part of the group
EXCLUDE_ACCOUNTS="admin,root"

# Security key to view audit logs
AUDIT_KEY="a_long_and_random_secret_string"
